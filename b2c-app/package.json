{
  "name": "b2c-app",
  "version": "5.4.0",
  "private": true,
  "overrides": {
    "@nguniversal/builders": {
      "@angular-devkit/build-angular": "$@angular-devkit/build-angular"
    },
    "@nguniversal/express-engine": {
      "@angular-devkit/build-angular": "$@angular-devkit/build-angular"
    }
  },
  "scripts": {
    "ng": "ng",
    "build:live": "ng build --configuration production --vendor-chunk --common-chunk --source-map && lite-server --baseDir dist",
    "build": "npm run env -s && ng build --configuration production --aot",
    "buildaot": "npm run env -s && ng build --aot",
    "prebuild": "npm --no-git-tag-version version patch",
    "start": "ng serve",
    "start:prod": "ng build --configuration production && lite-server --baseDir dist",
    "serve:sw": "npm run build -s && npx http-server ./dist -p 4200",
    "lint": "ng lint && stylelint \"src/**/*.scss\" --syntax scss && htmlhint \"src\" --config .htmlhintrc",
    "test": "npm run env -s && ng test",
    "test:ci": "npm run env -s && npm run lint -s && ng test --configuration=ci",
    "e2e": "npm run env -s && ng e2e",
    "translations:extract": "ngx-translate-extract --input ./src --output ./src/translations/template.json --format=json --clean --sort",
    "docs": "hads ./docs -o",
    "env": "ngx-scripts env npm_package_version",
    "prettier": "prettier --write \"./{src,e2e}/**/*.{ts,js,html,scss}\"",
    "prettier:check": "prettier --list-different \"./{src,e2e}/**/*.{ts,js,html,scss}\"",
    "generate": "ng generate",
    "build:ssr": "ng build --configuration=production && ng run b2c-app:server:production",
    "build:ssr:fast": "ng build --configuration=production --source-map=false --build-optimizer=false --named-chunks=false --vendor-chunk=false && ng run b2c-app:server:production --source-map=false",
    "serve:ssr": "node dist/server/main.js",
    "serve:ssr:prod": "cross-env NODE_ENV=production node dist/server/main.js",
    "prerender": "ng run b2c-app:prerender",
    "build:ssr:dev": "ng build --configuration development && ng run b2c-app:server:development",
    "serve:ssr:dev": "node dist/server/main.js",
    "build:client:watch": "ng build --configuration development --watch",
    "build:server:watch": "node --max-old-space-size=4096 ./node_modules/@angular/cli/bin/ng run b2c-app:server:development --watch",
    "serve:dev:ssr": "concurrently \"npm run build:client:watch\" \"npm run build:server:watch\" \"nodemon\"",
    "serve:debug:ssr": "concurrently \"npm run build:client:watch\" \"npm run build:server:watch\" \"nodemon --inspect dist/server/main.js\""
  },
  "dependencies": {
    "@angular/animations": "^18.2.13",
    "@angular/cdk": "^16.2.14",
    "@angular/common": "^18.2.13",
    "@angular/compiler": "^18.2.13",
    "@angular/core": "^18.2.13",
    "@angular/fire": "^18.0.1",
    "@angular/forms": "^18.2.13",
    "@angular/localize": "^18.2.13",
    "@angular/material": "^16.2.14",
    "@angular/platform-browser": "^18.2.13",
    "@angular/platform-browser-dynamic": "^18.2.13",
    "@angular/platform-server": "^18.2.13",
    "@angular/router": "^18.2.13",
    "@angular/service-worker": "^18.2.13",
    "@angular/ssr": "^18.2.13",
    "@aws-sdk/client-lambda": "^3.787.0",
    "@iterable/web-sdk": "^0.1.0",
    "@ng-bootstrap/ng-bootstrap": "^11.0.1",
    "@ng-idle/core": "^16.0.0",
    "@ng-idle/keepalive": "^16.0.0",
    "@nguniversal/builders": "^16.2.0",
    "@nguniversal/express-engine": "^16.2.0",
    "@ngx-translate/core": "^16.0.0",
    "@popperjs/core": "^2.11.8",
    "@swimlane/ngx-datatable": "^20.1.0",
    "@types/accept-language-parser": "^1.5.1",
    "@types/uuid": "^10.0.0",
    "ajv": "^8.17.1",
    "aws-sdk": "^2.1345.0",
    "bootstrap": "^4.5.2",
    "bowser": "^2.11.0",
    "buttercms": "^1.2.7",
    "cookie-parser": "^1.4.7",
    "dotenv": "^17.0.0",
    "express": "^4.21.2",
    "font-awesome": "^4.7.0",
    "google-libphonenumber": "^3.2.25",
    "html2canvas": "^1.4.1",
    "ics": "^3.8.1",
    "intl-tel-input": "^19.0.0",
    "jquery": "^3.5.1",
    "js-base64": "^3.7.5",
    "jspdf": "^2.5.1",
    "lz-string": "^1.5.0",
    "marked": "^12.0.2",
    "newrelic": "^11.0.0",
    "ngx-bootstrap": "^19.0.2",
    "ngx-cookie-service": "^18.0.0",
    "ngx-intl-tel-input": "^16.0.1",
    "ngx-markdown": "^18.0.0",
    "ngx-mask": "^14.3.3",
    "ngx-owl-carousel-o": "^18.0.1",
    "ngx-print": "^1.2.0",
    "ngx-skeleton-loader": "^8.0.0",
    "ngx-webstorage": "^18.0.0",
    "popper.js": "^1.16.1",
    "rxjs": "^6.6.3",
    "save": "^2.9.0",
    "serializer.ts": "0.0.12",
    "sweetalert2": "^11.21.0",
    "ts-loader": "^9.5.2",
    "tslib": "^2.0.0",
    "uuid": "^11.1.0",
    "uuidv4": "^6.2.6",
    "zone.js": "~0.14.10"
  },
  "devDependencies": {
    "@angular-builders/jest": "^10.0.0",
    "@angular-devkit/build-angular": "^18.2.14",
    "@angular/cli": "^18.2.14",
    "@angular/compiler-cli": "^18.2.13",
    "@angular/language-service": "^18.2.13",
    "@angularclass/hmr": "^3.0.0",
    "@biesbjerg/ngx-translate-extract": "^7.0.3",
    "@biesbjerg/ngx-translate-extract-marker": "^1.0.0",
    "@ngx-rocket/scripts": "^4.0.0",
    "@types/dotenv": "^6.1.1",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "^2.0.8",
    "@types/jest": "^26.0.12",
    "@types/lodash": "4.14.178",
    "@types/marked": "^6.0.0",
    "@types/node": "^20.0.0",
    "codelyzer": "^6.0.0",
    "concurrently": "^9.1.2",
    "hads": "^2.1.0",
    "htmlhint": "^0.14.1",
    "https-proxy-agent": "^5.0.0",
    "husky": "^4.3.8",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "jest": "^29.7.0",
    "lite-server": "^2.6.1",
    "nodemon": "^3.1.10",
    "prettier": "^2.1.1",
    "pretty-quick": "^3.0.0",
    "protractor": "~7.0.0",
    "stylelint": "^13.13.1",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-recommended-scss": "~4.2.0",
    "stylelint-config-standard": "~20.0.0",
    "stylelint-scss": "~3.18.0",
    "ts-node": "^9.0.0",
    "tslint": "~6.1.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "~5.4.5"
  },
  "prettier": {
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.scss",
        "options": {
          "singleQuote": false
        }
      }
    ]
  }
}
